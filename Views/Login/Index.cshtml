@model LoginViewModel

@{
ViewData["Title"] = "Login";
}

<div>
  <form action="/login" method="post">
    <p>
      <label>Email</label>
      <input name="Email" type="text" value="@Model.Email" />

    </p>
    <p>
      <label>Пароль</label>
      <input name="Password" type="password" />
    </p>
    <button>Войти</button>
  </form>
</div>
